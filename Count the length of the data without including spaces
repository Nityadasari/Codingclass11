import java.util.*;
class Coding2
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        int sum=0;
        System.out.println("Enter String:");
        String str=sc.nextLine();
        String ch[]=str.split(" ");
        for(int i=0;i<ch.length;i++)
        {
            String s=ch[i];
            sum+=s.length();
        }
        System.out.println(sum);
    }
}
